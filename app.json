{
  "expo": {
    "name": "roll-call",
    "slug": "roll-call",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "rollcall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "v",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSContactsUsageDescription": "RollCall needs access to your contacts to help you stay in touch."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "",
        "backgroundImage": "",
        "monochromeImage": ""
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.landonroney.rollcall",
      "permissions": ["READ_CONTACTS"]
    },
    "web": {
      "output": "static",
      "favicon": "",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "RollCall needs access to your contacts to help you stay in touch."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e6d71b63-9640-46fe-bf85-ab7d04f29499"
      }
    }
  }
}
